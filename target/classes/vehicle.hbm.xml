<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="it.cyberdyne.dss.vehicles.Vehicle" catalog="DSS" table="Vehicles">
        <id name="id" type="java.lang.Integer" column="id">
            <generator class="identity"/>
        </id>
        <property name="code" type="string" column="code"/>
        <property name="quantity" type="int" column="quantity"/>
        <property name="model" type="string" column="model"/>
        <property name="capacity" type="double" column="capacity"/>
        <property name="distance" type="double" column="distance"/>
        <property name="start" type="string" column="start"/>
        <property name="userId" type="int" column="userId"/>
        <property name="enabled" type="boolean" column="enabled"/>
        <property name="time" type="double" column="time"/>
        
    </class>
</hibernate-mapping>
